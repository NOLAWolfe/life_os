{
  "name": "vantage-os-ui",
  "private": true,
  "version": "3.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:ui\" \"npm run dev:server\"",
    "dev:ui": "vite",
    "dev:server": "node server.js",
    "build": "vite build",
    "lint": "eslint .",
    "format": "prettier --write .",
    "preview": "vite preview",
    "test": "cross-env NODE_ENV=test npx playwright test",
    "test:unit": "vitest",
    "test:ui": "vitest --ui",
    "check:all": "npm run lint && npm run test:unit -- run",
    "prepare": "husky",
    "start:server": "node server.js",
    "record:server": "npx appmap-node node server.js",
    "record:suite": "node scripts/record_suite.js"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-query": "^5.90.12",
    "@xyflow/react": "^12.10.0",
    "chart.js": "^4.5.1",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "moment": "^2.30.1",
    "prisma": "^5.22.0",
    "react": "^19.2.0",
    "react-big-calendar": "^1.19.4",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^19.2.0",
    "react-dropzone": "^14.3.8",
    "react-router-dom": "^7.10.1",
    "tailwindcss": "^4.1.18",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.57.0",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/ui": "^4.0.16",
    "appmap-node": "^2.24.3",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jsdom": "^27.3.0",
    "prettier": "^3.7.4",
    "vite": "npm:rolldown-vite@7.2.5",
    "vitest": "^4.0.16"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.5"
  }
}
